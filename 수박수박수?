#include <string>

#include <vector>

​

using namespace std;

​

string solution(int n)

{

string answer = "";

for (int i = 0; i < n; i++)

{

if(i%2==0)

answer.insert(answer.length(), "수");

else 

answer.insert(answer.length(), "박");

​

}

return answer;

}
