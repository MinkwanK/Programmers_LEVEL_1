#include <string>

#include <vector>

#include <iostream>

#include <cctype>

using namespace std;

​

bool solution(string s)

{

bool answer = true;

​

​

​

if (s.size() == 4 || s.size() == 6)

{

for (int i = 0; i < s.size(); i++)

{

​

if (!isdigit(s[i]))

{

answer = false;

return answer;

}

​

}

}

else

answer = false;

​

return answer;

​

}
